pragma solidity 0.8.19;
import "./Ownable.sol";
import "./IERC20.sol";
import "./IERC721.sol";
import "./ERC721AO.sol";
import "./MarketplaceFactoryEnabled.sol";
contract HumanMade is Ownable, ERC721AO, MarketplaceFactoryEnabled
{
    uint public _TOTAL_SUPPLY = 42;
    string public baseURI = "ipfs://QmaJKAsiEY4xp2BDosPzhR5nbEQz5ouDzaAwgGXwXPw74e/";
    mapping(address => bool) public Admin;

    constructor() ERC721AO("Human Made | Ntent x Bright Moments | HMMD", "HMMD") 
    { 
        // Thank You For Supporting The Project! (Initial Mint Pass Minters)
        _mint(0x5772A94eE0304325626371c8701a0dA825bfdF04, 1);
        _mint(0x32C610488fd0815CE21918AfB321ab935750574B, 1);
        _mint(0x1afA37f7aa5b8d54FDc476D7E0A27BD9F1039CC5, 1);
        _mint(0x4cE00c2e1abE73a409813D50fc3ac528A76394e2, 1);
        _mint(0x5772A94eE0304325626371c8701a0dA825bfdF04, 1);
        _mint(0xdbAc2e0D751db13FfB4dDF3174525F8EFaE8eca1, 1);
        _mint(0x76963eE4C482fA4F9E125ea3C9Cc2Ea81fe8e8C6, 1);
        _mint(0x76963eE4C482fA4F9E125ea3C9Cc2Ea81fe8e8C6, 1);
        _mint(0x20c485b466b233f5a54DBC2804fff8ad8c4c5384, 1);
        _mint(0x7FCbCa1C8C3E36c78Dfb67cc7DF0b71d1c201703, 1);
        _mint(0x75b3369B5a4093C5710719f1dBd1F8BE0EFAD3ec, 1);
        _mint(0xc26b9BFb82484f257E15144E750E16E30205d091, 1);
        _mint(0x101419E2d84470d900C41B86974B5912C92f01FE, 1);
        _mint(0x38E27a59d3cffB945aC8d41b7c398618354c08F6, 1);
        _mint(0x205798065bE2bf10Ba02A2303396b38beFDc3c06, 1);
        _mint(0x4D1C149e6728314d0d6C4f7d48DbF83bD196444E, 1);
        _mint(0x7FCbCa1C8C3E36c78Dfb67cc7DF0b71d1c201703, 1);
    }

    /**
     * @dev Factory Mint
     */
    function _MintToFactory(uint MintPassProjectID, address Recipient, uint Amount) override virtual external onlyMarketplace
    {
        require(totalSupply() + Amount <= _TOTAL_SUPPLY, "MP: Max Supply Reached");
        _mint(Recipient, Amount); 
    }

    /**
     * @dev Changes The Total Supply
     */
    function __ChangeTotalSupply(uint NewSupply) external onlyAdmin { _TOTAL_SUPPLY = NewSupply; }

    /**
     * @dev Changes The Marketplace Address
     */
    function __ChangeMarketplaceAddress(address NewAddress) override virtual external onlyAdmin { _MARKETPLACE_ADDRESS = NewAddress; }

    /**
     * @dev Changes The BaseURI For JSON Metadata 
     */
    function ___NewBaseURI(string calldata NewURI) external onlyAdmin { baseURI = NewURI; }

    /**
     * @dev Burns Golden Token(s)
     */
    function ___OwnerBurn(uint[] calldata TokenIDs) external onlyAdmin { for(uint x; x < TokenIDs.length; x++){ _burn(TokenIDs[x], false); } }

    /**
     * @dev Withdraws All Ether From The Contract
     */
    function ___WithdrawEther() external onlyAdmin { payable(msg.sender).transfer(address(this).balance); }

    /**
     * @dev Withdraws Ether From Contract To Address With An Amount
     */
    function ___WithdrawEtherToAddress(address payable Recipient, uint Amount) external onlyAdmin
    {
        require(Amount > 0 && Amount <= address(this).balance, "Invalid Amount");
        (bool Success, ) = Recipient.call{value: Amount}("");
        require(Success, "Unable to Withdraw, Recipient May Have Reverted");
    }

    /**
     * @dev Withdraws ERC20 Tokens
     **/
    function ___WithdrawERC20(address TokenAddress) external onlyAdmin 
    { 
        IERC20 erc20Token = IERC20(TokenAddress);
        uint balance = erc20Token.balanceOf(address(this));
        require(balance > 0, "0 ERC20 Balance At `TokenAddress`");
        erc20Token.transfer(msg.sender, balance);
    }

    /**
     * @dev Withdraws ERC721(s) Mistakenly Sent To Contract, From The Contract
     */
    function ___WithdrawERC721(address Contract, address Recipient, uint[] calldata TokenIDs) external onlyAdmin 
    { 
        for(uint TokenID; TokenID < TokenIDs.length; TokenID++)
        {
            IERC721(Contract).transferFrom(address(this), Recipient, TokenIDs[TokenID]);
        }
    }

    /**
     * @dev Flips Admin State
     */
    function ___FlipAdminState(address Wallet) external onlyOwner { Admin[Wallet] = !Admin[Wallet]; }
    
    /**
     * @dev Returns Base URI
     */
    function _baseURI() internal view virtual override returns (string memory) { return baseURI; }

    /**
     * @dev onlyAdmin Modifier
     */
    modifier onlyAdmin
    {
        require(Admin[msg.sender], "MP: msg.sender Is Not Admin");
        _;
    }
}