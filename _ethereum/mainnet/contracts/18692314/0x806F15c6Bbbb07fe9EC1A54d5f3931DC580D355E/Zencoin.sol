    uint256 public constant MAX_QUANTITY = 500;

    uint256 public constant BASE_TOKEN_PRICE = 2_000_000_000_000_000;
    uint256 public constant GAS_PRICE_COEFF = 200_000_000_000;

    uint256 public immutable genesisTs;
    uint256 public globalRank = GENESIS_RANK;
    uint256 public activeMinters;
    mapping(address => MintInfo) public userMints;

    address public mintingRewards;
    address public fastgas;

    Allocation[] public allocations;

    bool public inited;

    constructor() Ownable(msg.sender) {
        genesisTs = block.timestamp;
    }

    function _calculateMaxTerm() private view returns (uint256) {
        if (globalRank > TERM_AMPLIFIER_THRESHOLD) {
            uint256 delta = globalRank
                .fromUInt()
                .log_2()
                .mul(TERM_AMPLIFIER.fromUInt())
                .toUInt();
            uint256 newMax = MAX_TERM_START + delta * SECONDS_IN_DAY;
            return Math.min(newMax, MAX_TERM_END);
        }
        return MAX_TERM_START;
    }

    function _penalty(uint256 secsLate) private pure returns (uint256) {
        uint256 daysLate = secsLate / SECONDS_IN_DAY;
        if (daysLate > WITHDRAWAL_WINDOW_DAYS - 1) return MAX_PENALTY_PCT;
        uint256 penalty = (uint256(1) << (daysLate + 3)) /
            WITHDRAWAL_WINDOW_DAYS -
            1;
        return Math.min(penalty, MAX_PENALTY_PCT);
    }

    function _calculateMintReward(
        uint256 cRank,
        uint256 term,
        uint256 maturityTs,
        uint256 amplifier,
        uint256 eeaRate,
        uint256 quantity
    ) private view returns (uint256) {
        uint256 penalty = 0;
        if (block.timestamp > maturityTs) {
            uint256 secsLate = block.timestamp - maturityTs;
            penalty = _penalty(secsLate);
        }
        int128 rankDeltaSum = 0;

        for (uint256 i = 0; i < quantity; i++) {
            uint256 rankDelta = Math.max(globalRank - cRank - i, 2);
            rankDeltaSum = rankDeltaSum + rankDelta.fromUInt().log_2();
        }
        uint256 EAA = (1_000 + eeaRate);
        uint256 reward = getGrossReward(rankDeltaSum, amplifier, term, EAA);
        if (penalty == 0) {
            return reward;
        }
        return (reward * (100 - penalty)) / 100;
    }

    function _cleanUpUserMint() private {
        delete userMints[_msgSender()];
        activeMinters--;
    }

    function _calculateRewardAmplifier() private view returns (uint256) {
        uint256 amplifierDecrease = (block.timestamp - genesisTs) /
            SECONDS_IN_DAY;
        if (amplifierDecrease < REWARD_AMPLIFIER_START) {
            return
                Math.max(
                    REWARD_AMPLIFIER_START - amplifierDecrease,
                    REWARD_AMPLIFIER_END
                );
        } else {
            return REWARD_AMPLIFIER_END;
        }
    }

    function _calculateEAARate() private view returns (uint256) {
        uint256 decrease = (EAA_PM_STEP * globalRank) / EAA_RANK_STEP;
        if (decrease > EAA_PM_START) return 0;
        return EAA_PM_START - decrease;
    }

    function _getMintPrice(uint256 quantity) private view returns (uint256) {
        if (fastgas == address(0)) return 0;
        int256 gasPrice = IGasPrice(fastgas).latestAnswer();
        uint256 incPrice = (BASE_TOKEN_PRICE * uint256(gasPrice)) /
            GAS_PRICE_COEFF;
        return (BASE_TOKEN_PRICE + incPrice) * quantity;
    }

    function getGrossReward(
        int128 rankDeltaSum,
        uint256 amplifier,
        uint256 term,
        uint256 eaa
    ) public pure returns (uint256) {
        int128 reward128 = rankDeltaSum
            .mul(amplifier.fromUInt())
            .mul(term.fromUInt())
            .mul(eaa.fromUInt());
        return reward128.div(uint256(1_000).fromUInt()).toUInt();
    }

    function getUserMint(address user) external view returns (MintInfo memory) {
        return userMints[user];
    }

    function getCurrentAMP() external view returns (uint256) {
        return _calculateRewardAmplifier();
    }

    function getCurrentEAAR() external view returns (uint256) {
        return _calculateEAARate();
    }

    function getCurrentMaxTerm() external view returns (uint256) {
        return _calculateMaxTerm();
    }

    function getMintPrice(uint256 quantity) external view returns (uint256) {
        return _getMintPrice(quantity);
    }

    function getPenalty(address user) external view returns (uint256) {
        MintInfo memory mintInfo = userMints[user];
        if (mintInfo.rank == 0) return 0;
        uint256 penalty = 0;
        if (block.timestamp > mintInfo.maturityTs) {
            uint256 secsLate = block.timestamp - mintInfo.maturityTs;
            penalty = _penalty(secsLate);
        }
        return penalty;
    }

    function getMintReward(address user) external view returns (uint256) {
        MintInfo memory mintInfo = userMints[user];
        if (mintInfo.rank == 0) return 0;
        uint256 rewardAmount = _calculateMintReward(
            mintInfo.rank,
            mintInfo.term,
            mintInfo.maturityTs,
            mintInfo.amplifier,
            mintInfo.eaaRate,
            mintInfo.quantity
        ) * 1 ether;
        return rewardAmount;
    }

    function claimRank(uint256 term, uint256 quantity) external payable {
        require(inited, "CRank: Not initialized");
        require(allocations.length > 0, "CRank: No allocations");
        uint256 termSec = term * SECONDS_IN_DAY;
        require(termSec > MIN_TERM, "CRank: Term less than min");
        require(
            termSec < _calculateMaxTerm() + 1,
            "CRank: Term more than current max term"
        );
        require(
            userMints[_msgSender()].rank == 0,
            "CRank: Mint already in progress"
        );
        require(quantity >= MIN_QUANTITY, "CRank: Quantity less than min");
        require(quantity <= MAX_QUANTITY, "CRank: Quantity more than max");
        require(
            msg.value == _getMintPrice(quantity),
            "CRank: Incorrect mint amount"
        );

        MintInfo memory mintInfo = MintInfo({
            user: _msgSender(),
            term: term,
            maturityTs: block.timestamp + termSec,
            rank: globalRank,
            amplifier: _calculateRewardAmplifier(),
            eaaRate: _calculateEAARate(),
            quantity: quantity,
            cost: msg.value
        });
        userMints[_msgSender()] = mintInfo;

        activeMinters++;
        globalRank = globalRank + quantity;
        emit RankClaimed(_msgSender(), term, globalRank, quantity);

        IMintingRewards(mintingRewards).addMinting(
            _msgSender(),
            msg.value,
            termSec
        );

        uint256 averge = msg.value / uint256(100);
        uint256 amount = msg.value;
        for (uint256 i = 0; i < allocations.length; i++) {
            uint256 allocationReward = averge *
                allocations[i].allocationPercentage;
            if (allocationReward > amount) {
                allocationReward = amount;
            }
            (bool success, ) = allocations[i].allocationAddress.call{
                value: allocationReward
            }("");
            require(
                success,
                "Zencoin: Transfer to allocation address failed"
            );
            amount -= allocationReward;
            if (amount == 0) {
                break;
            }
        }
    }

    function claimMintReward() external {
        MintInfo memory mintInfo = userMints[_msgSender()];
        require(mintInfo.rank > 0, "CRank: No mint exists");
        require(
            block.timestamp > mintInfo.maturityTs,
            "CRank: Mint maturity not reached"
        );

        uint256 rewardAmount = _calculateMintReward(
            mintInfo.rank,
            mintInfo.term,
            mintInfo.maturityTs,
            mintInfo.amplifier,
            mintInfo.eaaRate,
            mintInfo.quantity
        ) * 1 ether;
        _mint(_msgSender(), rewardAmount);

        _cleanUpUserMint();
        emit MintClaimed(_msgSender(), rewardAmount);

        IMintingRewards(mintingRewards).endMinting(_msgSender());
    }

    function claimMintRewardAndShare(address other, uint256 pct) external {
        MintInfo memory mintInfo = userMints[_msgSender()];
        require(other != address(0), "CRank: Cannot share with zero address");
        require(pct > 0, "CRank: Cannot share zero percent");
        require(pct < 101, "CRank: Cannot share 100+ percent");
        require(mintInfo.rank > 0, "CRank: No mint exists");
        require(
            block.timestamp > mintInfo.maturityTs,
            "CRank: Mint maturity not reached"
        );

        uint256 rewardAmount = _calculateMintReward(
            mintInfo.rank,
            mintInfo.term,
            mintInfo.maturityTs,
            mintInfo.amplifier,
            mintInfo.eaaRate,
            mintInfo.quantity
        ) * 1 ether;
        uint256 sharedReward = (rewardAmount * pct) / 100;
        uint256 ownReward = rewardAmount - sharedReward;

        _mint(_msgSender(), ownReward);
        _mint(other, sharedReward);

        _cleanUpUserMint();
        emit MintClaimed(_msgSender(), rewardAmount);

        IMintingRewards(mintingRewards).endMinting(_msgSender());
    }

    function setup(
        address _mintingRewards,
        address _stakingRewards,
        address _fastgas
    ) external onlyOwner {
        allocations.push(
            Allocation({
                allocationAddress: _mintingRewards,
                allocationPercentage: uint256(80)
            })
        );
        allocations.push(
            Allocation({
                allocationAddress: _stakingRewards,
                allocationPercentage: uint256(20)
            })
        );
        mintingRewards = _mintingRewards;
        fastgas = _fastgas;
        inited = true;
    }

    function setAllocations(
        address[] memory _allocationAddresses,
        uint256[] memory _allocationPercentages
    ) external onlyOwner {
        require(_allocationAddresses.length > 0, "Zencoin: No allocations");
        require(
            _allocationAddresses.length == _allocationPercentages.length,
            "Zencoin: Arrays length mismatch"
        );

        uint256 totalPercentage;
        for (uint256 i = 0; i < _allocationAddresses.length; i++) {
            require(
                _allocationAddresses[i] != address(0),
                "Zencoin: Allocation address cannot be zero"
            );
            require(
                _allocationPercentages[i] > 0,
                "Zencoin: Allocation percentage cannot be zero"
            );
            totalPercentage += _allocationPercentages[i];
        }
        require(
            totalPercentage == 100,
            "Zencoin: Total allocation percentage must be 100"
        );

        delete allocations;

        for (uint256 i = 0; i < _allocationAddresses.length; i++) {
            allocations.push(
                Allocation({
                    allocationAddress: _allocationAddresses[i],
                    allocationPercentage: _allocationPercentages[i]
                })
            );
        }
    }
}