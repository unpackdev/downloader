// File contracts/RoyaltyReceiver.sol


pragma solidity ^0.8.9;


interface ILuckBound is IERC721A {
  function getBoundTokens(uint8 score) external view returns(uint[] memory);
  function getBoundTokensLength(uint8 score) external view returns(uint);
  function pack(uint score, uint index) external view returns(uint);
  function totalBoundScore() external view returns(uint);
}

contract RoyaltyReceiver {
	uint nonce = 0;
	ILuckBound public luckbound;
	address public dev;

	constructor(address luckbound_) {
		luckbound = ILuckBound(luckbound_);
		dev = msg.sender;
	}

	receive() external payable {

	}

	function setDev(address newDev_) external {
		require(msg.sender == dev, "Not dev");
		dev = newDev_;
	}

	function random(uint256 seed) internal view returns (uint256) {
	  return uint256(keccak256(abi.encodePacked(tx.origin, blockhash(block.number - 1), block.timestamp, seed)));
	}

	function distribute(address token) external {
	    require(msg.sender == tx.origin, "Only EOA");
	    address randomRecipient = luckyRecipient();
	    if(randomRecipient == address(0x0)) {
	        return;
	    }
	    uint etherBalance = address(this).balance;
        uint creatorFee = etherBalance * 80 / 100;
        payable(randomRecipient).transfer(creatorFee);
        payable(dev).transfer(etherBalance - creatorFee);

        randomRecipient = luckyRecipient();
        if(randomRecipient == address(0x0)) {
            return;
        }
        uint balance = IERC20(token).balanceOf(address(this));
        if(balance > 0) {
            IERC20(token).transfer(randomRecipient, balance * 80 / 100);
            IERC20(token).transfer(dev, balance - balance * 80 / 100);
        }
	}

	function luckyRecipient() internal returns(address) {
	  uint seed = random(++nonce);
	  uint bucket = (seed & 0xFFFFFFFF) % luckbound.totalBoundScore();
	  uint cumulative;
	  seed >>= 32;
	  for (uint i = 5; i <= 8; i++) {
	    cumulative += luckbound.getBoundTokensLength(uint8(i)) * i;
	    if (bucket >= cumulative) continue;
	    return luckbound.ownerOf(luckbound.pack(uint8(i), seed % luckbound.getBoundTokensLength(uint8(i))));
	  }
	  return address(0x0);
	}
}