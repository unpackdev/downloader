/********************
 *   A lender plugin for LenderYieldOptimiser for USDC on Interest Protocol
 *   Made by Jake B @ GFX labs
 *   https://gfx.cafe/ip/genericinterestprotocolstrategy
 *
 ********************* */
contract GenericInterestProtocol is GenericLenderBase {
    using SafeERC20 for IERC20;

    IUSDI public constant USDi =
        IUSDI(0x2A54bA2964C8Cd459Dc568853F79813a60761B58);
    IVaultController public constant VaultController =
        IVaultController(0x4aaE9823Fb4C70490F1d802fC697F3ffF8D5CbE3);
    bool private initialized;

    // initialization logic
    constructor(address _strategy, string memory name)
        GenericLenderBase(_strategy, name)
    {
        _initialize();
    }

    function initialize() external {
        _initialize();
    }

    function _initialize() internal {
        require(!initialized, "Already initialized");
        require(
            USDi.reserveAddress() == address(want),
            "USDi reserve matches want"
        );
        want.safeApprove(address(USDi), 2**256 - 1);
        initialized = true;
    }

    // clones an ip lender
    function clone_IP_lender(address _strategy, string memory _name)
        external
        returns (address newLender)
    {
        newLender = _clone(_strategy, _name);
        GenericInterestProtocol(newLender).initialize();
    }

    // gets the apr
    function apr() external view override returns (uint256) {
        return _apr();
    }

    // gets the borrow apr
    function borrowAPR() external view returns (uint256) {
        return _borrowAPR(0);
    }

    function _borrowAPR(uint256 inputReserveRatio)
        internal
        view
        returns (uint256)
    {
        // get curve master
        ICurveMaster curve = ICurveMaster(VaultController.getCurveMaster());
        int256 reserve_ratio;
        if (inputReserveRatio == 0) {
            // the current reserve ratio, cast to a uint256
            uint256 ui18 = uint256(USDi.reserveRatio());
            // cast the reserve ratio now to an int in order to get a curve value
            reserve_ratio = int256(ui18);
        } else {
            // simply case the input value to an int
            reserve_ratio = int256(inputReserveRatio);
        }
        // get the curveValue at 0x00, aka the default curve
        int256 intCurveVal = curve.getValueAt(address(0x00), reserve_ratio);
        // cast the integer curve value to a u192
        uint192 curveVal = safeu192(uint256(intCurveVal));

        // the apr is equal to the curve value
        return uint256(curveVal);
    }

    function safeu192(uint256 u) internal pure returns (uint192) {
        require(u < 2**192 - 1, "overflow");
        return uint192(u);
    }

    // the deposit apr
    function _apr() internal view returns (uint256) {
        uint256 depositRate = _calcDepositRate(
            _borrowAPR(0),
            USDi.reserveRatio(),
            VaultController.protocolFee()
        );
        return depositRate;
    }

    ///@param amount in terms of WANT (USDC = 1e6)
    function aprAfterDeposit(uint256 amount)
        external
        view
        override
        returns (uint256)
    {
        uint256 predicted = predictReserveRatio(amount);
        uint256 depositRate = _calcDepositRate(
            _borrowAPR(predicted),
            predicted,
            VaultController.protocolFee()
        );
        return depositRate;
    }

    //the deposit rate = borrow rate * (1-reserve ratio) * (1-protocolFee)
    //IP stores decimals by multiplying by 1e18,
    function _calcDepositRate(
        uint256 borrow_rate,
        uint256 reserve_ratio,
        uint256 protocol_fee
    ) internal pure returns (uint256) {
        return
            ((borrow_rate) * (1e18 - reserve_ratio) * (1e18 - protocol_fee)) /
            1e36;
    }

    ///@param amount in terms of WANT (USDC = 1e6)
    function predictReserveRatio(uint256 amount)
        internal
        view
        returns (uint256 predicted)
    {
        uint256 reserve = want.balanceOf(address(USDi));
        uint256 newReserve = reserve + amount;
        uint256 newSupply = USDi.totalSupply() + (amount * 1e12);

        predicted = ((newReserve * 1e18) / newSupply) * 1e12;
    }

    function deposit() external override management {
        // get the full balance USDC in the contract
        uint256 balance = want.balanceOf(address(this));
        // deposit that much into USDi
        USDi.deposit(balance);
    }

    ///@param amount in terms of WANT (USDC = 1e6)
    function emergencyWithdraw(uint256 amount)
        external
        override
        onlyGovernance
    {
        // withdraw amount (in usdc) from USDi
        USDi.withdraw(amount);
        // transfer that amount to the governance contract
        want.safeTransfer(vault.governance(), want.balanceOf(address(this)));
    }

    // check if either balance of both USDi and USDC are not 0
    function hasAssets() external view override returns (bool) {
        return
            USDi.balanceOf(address(this)) > 0 ||
            want.balanceOf(address(this)) > 0;
    }

    function nav() external view override returns (uint256) {
        return _nav();
    }

    /// return in terms of WANT (USDC = 1e6)
    function _nav() internal view returns (uint256) {
        return
            want.balanceOf(address(this)) +
            (USDi.balanceOf(address(this)) / 1e12);
    }

    function protectedTokens()
        internal
        view
        override
        returns (address[] memory)
    {
        address[] memory protected = new address[](1);
        protected[0] = address(want);
        return protected;
    }

    // the unscaped product  of _apr() and _nav()
    function weightedApr() external view override returns (uint256) {
        uint256 a = _apr();
        return a * _nav();
    }

    ///@param amount in terms of WANT (USDC = 1e6)
    function withdraw(uint256 amount)
        external
        override
        management
        returns (uint256)
    {
        return _withdraw(amount);
    }

    ///@param amount in terms of WANT (USDC = 1e6)
    function _withdraw(uint256 amount) internal returns (uint256) {
        // get the USDi balance of the contract
        uint256 balanceUnderlying = USDi.balanceOf(address(this));
        // get the loose balance of USDC in the contract
        uint256 looseBalance = want.balanceOf(address(this));

        ///total is the total value in terms of WANT (USDC = 1e6)
        /// e.g if we had 1 usdc and 1e12 USDi, we would have 2 total, since USDi is 18 decimals, and 18 - 6 = 12
        uint256 total = (balanceUnderlying / 1e12) + looseBalance;

        if (amount > total) {
            //cant withdraw more than we own, so withdraw as much as we can
            amount = total;
        }

        //if we have enough looseBalance laying around, yay! just send that and finish!
        if (looseBalance >= amount) {
            want.safeTransfer(address(strategy), amount);
            return amount;
        }

        //how much want(usdc) is available from the lender (in IP reserve)?
        uint256 liquidity = want.balanceOf(address(USDi));

        // if there is no liquidity, there is no usdi to withdraw
        if (liquidity > 1) {
            uint256 toWithdraw = amount - looseBalance;
            ///@notice IP does not allow a withdraw of 0

            if (toWithdraw <= liquidity) {
                //we can take all
                USDi.withdraw(toWithdraw);
            } else {
                //take what we can get
                USDi.withdraw(liquidity);
            }
        }
        // the loose balance is now greater, and should be equal to the amount requested
        looseBalance = want.balanceOf(address(this));
        // so send the total loose balance.
        want.safeTransfer(address(strategy), looseBalance);

        return looseBalance;
    }

    function withdrawAll() external override management returns (bool) {
        uint256 invested = _nav();
        uint256 returned = _withdraw(invested);
        return returned >= invested;
    }
}