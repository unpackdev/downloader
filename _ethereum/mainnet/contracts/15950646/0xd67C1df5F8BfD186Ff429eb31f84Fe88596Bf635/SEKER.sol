contract SEKER is ERC20, Ownable {
    constructor() ERC20("Seker Factory DAO", "SEKER") {
        _transferOwnership(address(0x950685620c3Fa521fF0B3e59BF7D1dac76731428));
        _mint(owner(), 21000000000000000000000000);
    }

    function mintDAO(uint256 _amount) public onlyOwner {
        _mint(owner(), _amount);
    }
}
