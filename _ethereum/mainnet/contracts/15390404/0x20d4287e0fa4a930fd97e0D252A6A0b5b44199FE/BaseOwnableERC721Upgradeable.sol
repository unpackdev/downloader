pragma solidity 0.8.13;
import "./ERC721AUpgradeable.sol";
import "./UUPSUpgradeable.sol";
import "./OwnableUpgradeable.sol";

/// @dev Use this contract as the base for contracts that are going to be ownable ERC721 and upgradeable..
abstract contract BaseOwnableERC721Upgradeable is
    OwnableUpgradeable,
    ERC721AUpgradeable,
    UUPSUpgradeable
{
    // solhint-disable-next-line func-name-mixedcase
    function __BaseOwnableERC721Upgradeable_init(
        string memory _tokenName,
        string memory _tokenSymbol
    ) internal initializerERC721A onlyInitializing {
        __Ownable_init();
        __ERC721A_init(_tokenName, _tokenSymbol);
        __UUPSUpgradeable_init();
    }

    // solhint-disable-next-line no-unused-vars no-empty-blocks
    function _authorizeUpgrade(address implementation)
        internal
        view
        override
        onlyOwner
    {}
}
