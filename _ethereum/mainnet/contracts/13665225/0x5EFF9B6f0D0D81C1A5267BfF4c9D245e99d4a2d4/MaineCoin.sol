pragma solidity ^0.8.0;

import "./ERC20.sol";
import "./ERC20Burnable.sol";

contract MaineCoin is ERC20, ERC20Burnable {

  constructor(address mintTo) ERC20("MaineCoin", "MACO") {
    _mint(mintTo, 10 * 10 ** 12 * 10 ** decimals());
  }

}