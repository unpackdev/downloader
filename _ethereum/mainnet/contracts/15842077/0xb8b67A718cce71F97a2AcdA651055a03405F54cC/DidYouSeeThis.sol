pragma solidity >=0.4.22 <0.9.0;
import "./ERC1155A.sol";

contract DidYouSeeThis is ERC1155A {
  address private _owner;
  constructor() ERC1155A("https://nft.didyouseethis.xyz/campaigns/000/metadata/{id}") {
    _owner = msg.sender;
  }

  function mint(
    address to,
    uint256 id,
    uint256 amount
  ) public {
    require(_owner == msg.sender, "not contract owner");
    _mint(to, id, amount);
  }

  function airMint(address[] calldata receivers, uint256 tokenId) public {
    require(_owner == msg.sender, "not contract owner");
    _airMint(receivers, tokenId, 1);
  }

}
