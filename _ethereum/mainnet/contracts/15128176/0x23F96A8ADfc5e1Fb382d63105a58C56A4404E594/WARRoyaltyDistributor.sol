contract WARRoyaltyDistributor is Context, Ownable {
    using SafeMath for uint256;
    address public devwallet = 0xCD0711eF765A3a39766ec4787eAAc143282ce345;
    address public charitywallet = 0x6ae25c63924AF0905c2e76BAaF41f9f661CBb78E; 
    uint256 public devfee = 20;
    uint256 public charityfee = 80;

    receive() external payable {
        distribute();
    }

    function distribute() internal {
        uint256 balance = address(this).balance;

        if(balance > 0) {
            uint256 tCharityFee = balance.mul(charityfee).div(10**2);
            uint256 tDevFee = balance.sub(tCharityFee);
            payable(charitywallet).transfer(tCharityFee);
            payable(devwallet).transfer(tDevFee);
        }
    }

    /**
     * @dev all fee received from each sell royalties will be distributed to charity and developer wallets
     */
    function distributeRoyalties() external onlyOwner {
        distribute();
    }

    function updateFeeReceivers(address _charityReceiver, address _devReceiver) external onlyOwner {
        charitywallet = _charityReceiver;
        devwallet = _devReceiver;
    }


    /**
     * @dev set distribution fee percent for charity and developer wallet
     */
    function udpateRoyaltiesPercent(uint256 _charityFeePercent, uint256 _devFeePercent) external onlyOwner {
        charityfee = _charityFeePercent;
        devfee = _devFeePercent;
    }

    /**
     * @dev claim eth available in the contract to admin wallet directly
     */
    function claimEth() external onlyOwner {
        payable(_msgSender()).transfer(address(this).balance);
    }
}