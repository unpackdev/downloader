pragma solidity 0.8.19;
import "./Ownable.sol";
contract BatchPurchaser is Ownable
{
    address public _MINTER = 0xB5C1F9c5e8032264a10e618fb64042E3C3CDf39c; // Minter Address
    address public _ESCROW = 0x5168D59eFEBF8B86513b8728EC3e2D9Aa099BD93; // Recipient Address
    uint public _ABProjectID = 20;                                       // ArtBlocks ProjectID

    /**
     * @dev Batch Mints 
     * @param Amount The Amount To Mint To Escrow
     */
    function BatchMint(uint Amount) external onlyOwner { for(uint x; x < Amount; x++) { IMinter(_MINTER).purchaseTo(_ESCROW, _ABProjectID); } }

    /**
     * @dev Changes The Minter Address
     * @param NewMinter The New Minter Address
     */
    function _ChangeMinter(address NewMinter) external onlyOwner { _MINTER = NewMinter; }

    /**
     * @dev Changes The Escrow Address
     * @param NewAddress The New Escrow Recipient Address
     */
    function _ChangeEscrow(address NewAddress) external onlyOwner { _ESCROW = NewAddress; }

    /**
     * @dev Changes ArtBlocks ProjectID
     * @param NewProjectID The ArtBlocks ProjectID To Overwrite
     */
    function _ChangeProjectID(uint NewProjectID) external onlyOwner { _ABProjectID = NewProjectID; }
}

interface IMinter { function purchaseTo(address _to, uint _projectId) payable external returns (uint tokenID); }