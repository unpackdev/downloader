pragma solidity 0.8.14;

import "ShatterV2.sol";

contract ShatteredOpportunities is ShatterV2 {

    constructor (
        address royaltyRecipient,
        uint256 royaltyPercentage,
        address admin,
        uint256 num,
        uint256 time
    )
    ShatterV2(
        "Shattered Opportunities",
        "SHOPP",
        royaltyRecipient,
        royaltyPercentage,
        admin,
        num,
        time
    )
    {}

}